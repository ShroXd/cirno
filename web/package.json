{
  "name": "flandre-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage --coverage.reporter=html",
    "test:update": "vitest run -u",
    "lint": "eslint ./src",
    "lint:files": "eslint ./src --format unix | cut -d: -f1 | sort -u",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css}' '!src/bindings/**'",
    "typecheck": "tsc --noEmit",
    "check": "pnpm typecheck && pnpm lint && pnpm test",
    "clean": "rm -rf dist",
    "analyze": "vite build --mode analyze"
  },
